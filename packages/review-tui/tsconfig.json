{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "outDir": "dist",
    "rootDir": "src",
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "strict": false,
    "skipLibCheck": true,
    "noEmit": true
  },
  "include": ["src/**/*"]
}
