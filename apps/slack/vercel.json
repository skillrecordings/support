{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "bunVersion": "1.x",
  "installCommand": "cd ../.. && bun install",
  "buildCommand": "cd ../.. && bun turbo run build --filter=slack",
  "ignoreCommand": "npx turbo-ignore",
  "build": {
    "env": {
      "SKIP_ENV_VALIDATION": "1"
    }
  }
}
